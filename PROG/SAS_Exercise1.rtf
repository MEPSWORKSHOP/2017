\par 49         /* READ IN DATA FROM 2014 CONSOLIDATED DATA FILE (HC-163) */
\par 50         DATA PUF171;
\par 51           SET CDATA.H171 (KEEP= TOTEXP14 IPDEXP14 IPFEXP14 OBVEXP14 RXEXP14
\par 52         	                      OPDEXP14 OPFEXP14 DVTEXP14 ERDEXP14 ERFEXP14
\par 53         	                      HHAEXP14 HHNEXP14 OTHEXP14 VISEXP14 AGE14X AGE42X AGE31X
\par 54         	                      VARSTR   VARPSU   PERWT14F );
\par 55
\par 56           /* Define expenditure variables by type of service  */
\par 57
\par 58           TOTAL                = TOTEXP14;
\par 59           HOSPITAL_INPATIENT   = IPDEXP14 + IPFEXP14;
\par 60           AMBULATORY           = OBVEXP14 + OPDEXP14 + OPFEXP14 + ERDEXP14 + ERFEXP14;
\par 61           PRESCRIBED_MEDICINES = RXEXP14;
\par 62           DENTAL               = DVTEXP14;
\par 63           HOME_HEALTH_OTHER    = HHAEXP14 + HHNEXP14 + OTHEXP14 + VISEXP14;
\par 64
\par 65          /*QC CHECK IF THE SUM OF EXPENDITURES BY TYPE OF SERVICE IS EQUAL TO TOTAL*/
\par 66
\par 67           DIFF = TOTAL-HOSPITAL_INPATIENT - AMBULATORY   - PRESCRIBED_MEDICINES
\par 68                       - DENTAL            - HOME_HEALTH_OTHER        ;
\par 69
\par 70          /* CREATE FLAG (1/0) VARIABLES FOR PERSONS WITH AN EXPENSE, BY TYPE OF SERVICE  */
\par 71
\par 72           ARRAY EXX  (6) TOTAL     HOSPITAL_INPATIENT   AMBULATORY     PRESCRIBED_MEDICINES
\par 73                          DENTAL    HOME_HEALTH_OTHER      ;
\par 74
\par 75           ARRAY ANYX (6) X_ANYSVCE X_HOSPITAL_INPATIENT X_AMBULATORY    X_PRESCRIBED_MEDICINES
\par 76                          X_DENTAL  X_HOME_HEALTH_OTHER    ;
\par 77
\par 78           DO II=1 TO 6;
\par 79             ANYX(II) = 0;
\par 80             IF EXX(II) > 0 THEN ANYX(II) = 1;
\par 81           END;
\par 82           DROP II;
\par 83
\par 84           /* CREATE A SUMMARY VARIABLE FROM END OF YEAR, 42, AND 31 VARIABLES*/
\par 85
\par 86                IF AGE14X >= 0 THEN AGE = AGE14X ;
\par 87           ELSE IF AGE42X >= 0 THEN AGE = AGE42X ;
\par 88           ELSE IF AGE31X >= 0 THEN AGE = AGE31X ;
\par 89
\par 90                IF 0 LE AGE LE 64 THEN AGECAT=1 ;
\par 91           ELSE IF      AGE  > 64 THEN AGECAT=2 ;
\par 92         RUN;
